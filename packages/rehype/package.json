  "description": "A Remark plugin for optimizing images with PicPerf.io.",
  "license": "ISC",
  "homepage": "https://picperf.io",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
