{
  "name": "@picperf/remark",
  "version": "0.0.0",
  "description": "A Remark plugin for optimizing images with PicPerf.dev.",
  "license": "ISC",
  "author": "Alex MacArthur (https://macarthur.me)",
  "homepage": "https://picperf.dev",
  "main": "dist/cjs/packages/remark/index.js",
  "module": "dist/esm/packages/remark/index.js",
  "types": "dist/types/packages/remark/index.d.ts",
  "keywords": ["image optimization", "picperf", "remark", "rehype", "unified"],
  "scripts": {
    "test": "vitest run",
    "build": "rm -rf dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --module es2020 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs"
  },
  "dependencies": {
    "@brillout/import": "^0.2.3",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "remark-html": "^15.0.2",
    "remark-parse": "^10.0.2",
    "remark-stringify": "^10.0.3",
    "unified": "^10.1.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
